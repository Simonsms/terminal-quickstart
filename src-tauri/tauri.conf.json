{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Terminal QuickStart",
  "version": "1.0.0",
  "identifier": "com.terminalquickstart.dev",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Terminal QuickStart - 终端快捷启动",
        "width": 960,
        "height": 680,
        "minWidth": 800,
        "minHeight": 600,
        "center": true,
        "decorations": true,
        "resizable": true
      }
    ],
    "security": {
      "csp": null,
      "capabilities": ["default"]
    }
  },
  "bundle": {
    "active": true,
    "targets": ["msi"],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "shortDescription": "终端命令快捷启动工具",
    "longDescription": "Terminal QuickStart 是一款便捷的终端命令管理工具，帮助开发者快速启动项目和执行常用命令。",
    "copyright": "© 2024 Terminal QuickStart",
    "category": "DeveloperTool",
    "publisher": "Terminal QuickStart"
  }
}
